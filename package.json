{
  "name": "portable-backend-boilerplate-graphql",
  "version": "1.0.0",
  "description": "Boilerplate for creating GraphQL applications",
  "author": "Jared Jewitt <jaredjewitt@gmail.com> (https://jared-jewitt.github.io/)",
  "license": "ISC",
  "keywords": [],
  "scripts": {
    "build": "babel src --out-dir=dist",
    "start": "cross-env NODE_ENV=development nodemon --exec babel-node src/server.js",
    "serve": "cross-env NODE_ENV=production npm run build && node dist/server.js",
    "db:seed": "cross-env NODE_ENV=development babel-node src/database/utils/seed.js",
    "db:wipe": "cross-env NODE_ENV=development babel-node src/database/utils/wipe.js",
    "test": "jest",
    "test:update": "jest --updateSnapshot",
    "test:coverage": "jest --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint --fix ."
  },
  "dependencies": {
    "apollo-datasource": "^0.6.4",
    "apollo-server": "^2.12.0",
    "bcryptjs": "^2.4.3",
    "custom-env": "^2.0.1",
    "graphql": "^14.6.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.9.10"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/node": "^7.8.7",
    "@babel/preset-env": "^7.9.5",
    "babel-eslint": "^10.1.0",
    "cross-env": "^6.0.3",
    "eslint": "^6.8.0",
    "eslint-plugin-jest": "^23.8.2",
    "jest": "^24.9.0",
    "nodemon": "^2.0.3"
  }
}
