{
  "name": "portable-graphql-boilerplate",
  "version": "1.0.0",
  "description": "Boilerplate for GraphQL projects",
  "author": "Jared Jewitt <jaredjewitt@gmail.com> (https://jared-jewitt.github.io/)",
  "license": "ISC",
  "keywords": [],
  "repository": {
    "type": "git",
    "url": "https://github.com/portable-boilerplates/portable-graphql-boilerplate.git"
  },
  "main": "src/server.js",
  "scripts": {
    "start": "cross-env NODE_ENV=development nodemon -L --exec babel-node -r dotenv/config src/server.js",
    "build": "babel src --out-dir dist",
    "serve": "cross-env NODE_ENV=production node -r dotenv/config dist/server.js",
    "test": "jest",
    "test:update": "jest --updateSnapshot",
    "test:coverage": "jest --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "seed-db": "echo \"TODO...\" "
  },
  "dependencies": {
    "apollo-datasource": "^0.6.4",
    "apollo-server": "^2.9.16",
    "bcryptjs": "^2.4.3",
    "graphql": "^14.5.8",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.8.9"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.3",
    "@babel/core": "^7.8.3",
    "@babel/node": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "babel-eslint": "^10.0.3",
    "cross-env": "^6.0.3",
    "dotenv": "^8.2.0",
    "eslint": "^6.8.0",
    "eslint-plugin-jest": "^23.6.0",
    "jest": "^24.9.0",
    "nodemon": "^2.0.2"
  }
}
